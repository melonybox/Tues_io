<h1>Hello <%= @logged_in_user.username %> </h1>

<ul>
  <% if @logged_in_user.tasks.empty? %>
    <%= "No current Tasks"%>
    <% else %>
    <% @logged_in_user.tasks.each do |task| %>
      <li><%= link_to task.description, project_path(task.project.id) %></li>
    <% end %>
  <% end %>
</ul>

