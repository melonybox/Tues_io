<h1>START NEW PROJECT!</h1>

<%= form_for(@project) do |f|%>
<%= f.hidden_field :owner_id, :value => @logged_in_user.id %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :synopsis %>
<%= f.text_field :synopsis %>
<%= f.label :deadline %>
<%= f.date_select :deadline %>
<%= f.submit %>
<% end %>